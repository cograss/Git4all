' Gambas class file

Public Sub _new()

End

Public Sub Form_Open()
  Me.Center
End


Public Sub menuSettingsNewAccount_Click()
 
 
  
  webForm.WebView1.Url = main.glbGithubUrl
  myFunctions.webFormMessages(("Download progress:"), ("GitHUB registration page"))

  webForm.Show

End

Public Sub Form_Close()

  webForm.Close
  
End

Public Sub Button1_Click()

  webForm.WebView1.HTML = myFunctions.openFile("<html><body>" & main.glbConfigPath & "</body></html>")
  webForm.Show

End
